      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []}
    },
    "route": {
      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []}
    },
    "geolocation": {
      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []}
    },
    "fromMarker": {
      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []}
    },
    "patrimoinemondialenfrance": {
      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []},
