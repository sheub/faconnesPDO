{
  "version": 8,
  "name": "OSM Basic",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "tiles": [
        "https://maps.tilehosting.com/data/v3/{z}/{x}/{y}.pbf?key=pErAY63Zg9cVHhUnLbku"
