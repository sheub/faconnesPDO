      "attribution": "<a href=\"https://www.openmaptiles.org/\" target=\"_blank\" rel=\"noopener\">&copy; OpenMapTiles</a> <a href=\"https://www.openstreetmap.org/about/\" target=\"_blank\" rel=\"noopener\">&copy; OpenStreetMap contributors</a>"
    },
    "localEvents": {
      "type": "vector",
      "tiles": ["https://faconnes.de/tiles/localEvents/{z}/{x}/{y}.pbf"],
      "maxzoom": 6
    },
    "localTips": {
      "type": "vector",
      "tiles": ["https://faconnes.de/tiles/localTips/{z}/{x}/{y}.pbf"],
